6.01057 5.53708 6.00551 6.00391 6.00391C6.00551 5.53708 6.01057 5.14051 6.03809 4.80371C6.0755 4.34588 6.15483 3.94012 6.34668 3.56348L6.46875 3.34473C6.77282 2.84912 7.20856 2.44514 7.72949 2.17969L7.87207 2.11328C8.20855 1.96886 8.56979 1.90385 8.9707 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-keyword">import</span></span><span> torch
</span><span><span class="hljs-keyword">import</span></span><span> torch.nn </span><span><span class="hljs-keyword">as</span></span><span> nn
</span><span><span class="hljs-keyword">import</span></span><span> torch.nn.functional </span><span><span class="hljs-keyword">as</span></span><span> F

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">ResidualMLP</span></span><span>(nn.Module):
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, dim, mlp_ratio=<span class="hljs-number">4.0</span></span></span><span>, dropout=</span><span><span class="hljs-number">0.0</span></span><span>):
        </span><span><span class="hljs-built_in">super</span></span><span>().__init__()
        hidden = </span><span><span class="hljs-built_in">int</span></span><span>(dim * mlp_ratio)
        self.fc1 = nn.Linear(dim, hidden)
        self.act = nn.GELU()
        self.fc2 = nn.Linear(hidden, dim)
        self.dropout = nn.Dropout(dropout)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">forward</span></span><span>(</span><span><span class="hljs-params">self, x</span></span><span>):
        x = self.fc1(x)
        x = self.act(x)
        x = self.dropout(x)
        x = self.fc2(x)
        x = self.dropout(x)
        </span><span><span class="hljs-keyword">return</span></span><span> x

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">AlternatingAttentionBlock</span></span><span>(nn.Module):
    <span class="hljs-string">"""
    One block that performs:
      - Intra-frame (spatial) self-attention on each frame independently
      - Inter-frame (temporal) attention at each spatial location across frames

    You can chain several of these blocks.
    """</span>

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, dim, num_heads, mlp_ratio=<span class="hljs-number">4.0</span></span></span><span>, dropout=</span><span><span class="hljs-number">0.0</span></span><span>, attn_dropout=</span><span><span class="hljs-number">0.0</span></span><span>, layer_norm_eps=</span><span><span class="hljs-number">1e-6</span></span><span>):
        </span><span><span class="hljs-built_in">super</span></span><span>().__init__()
        self.dim = dim
        self.num_heads = num_heads

        </span><span><span class="hljs-comment"># For spatial (intra-frame) attention</span></span><span>
        self.norm1_spatial = nn.LayerNorm(dim, eps=layer_norm_eps)
        self.attn_spatial = nn.MultiheadAttention(embed_dim=dim, num_heads=num_heads, dropout=attn_dropout, batch_first=</span><span><span class="hljs-literal">False</span></span><span>)
        self.mlp_spatial = ResidualMLP(dim, mlp_ratio, dropout)
        self.norm2_spatial = nn.LayerNorm(dim, eps=layer_norm_eps)

        </span><span><span class="hljs-comment"># For temporal (inter-frame) attention</span></span><span>
        self.norm1_temporal = nn.LayerNorm(dim, eps=layer_norm_eps)
        self.attn_temporal = nn.MultiheadAttention(embed_dim=dim, num_heads=num_heads, dropout=attn_dropout, batch_first=</span><span><span class="hljs-literal">False</span></span><span>)
        self.mlp_temporal = ResidualMLP(dim, mlp_ratio, dropout)
        self.norm2_temporal = nn.LayerNorm(dim, eps=layer_norm_eps)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">forward</span></span><span>(</span><span><span class="hljs-params">self, x</span></span><span>):
        </span><span><span class="hljs-comment"># x: (B, T, N, C)</span></span><span>
        B, T, N, C = x.shape
        </span><span><span class="hljs-keyword">assert</span></span><span> C == self.dim

        </span><span><span class="hljs-comment"># ---- Intra-frame (spatial) self-attention ----</span></span><span>
        </span><span><span class="hljs-comment"># Normalize</span></span><span>
        xs = self.norm1_spatial(x)  </span><span><span class="hljs-comment"># (B, T, N, C)</span></span><span>
        </span><span><span class="hljs-comment"># reshape to (B*T, N, C) then to (N, B*T, C) for MultiheadAttention</span></span><span>
        xs = xs.view(B * T, N, C).permute(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>)  </span><span><span class="hljs-comment"># (N, B*T, C)</span></span><span>
        </span><span><span class="hljs-comment"># self-attend (query=key=value for self-attention)</span></span><span>
        attn_out_spatial, _ = self.attn_spatial(xs, xs, xs)  </span><span><span class="hljs-comment"># (N, B*T, C)</span></span><span>
        attn_out_spatial = attn_out_spatial.permute(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>).contiguous().view(B, T, N, C)  </span><span><span class="hljs-comment"># (B, T, N, C)</span></span><span>
        </span><span><span class="hljs-comment"># Residual + MLP</span></span><span>
        x = x + attn_out_spatial
        x = x + self.mlp_spatial(self.norm2_spatial(x))

        </span><span><span class="hljs-comment"># ---- Inter-frame (temporal) attention ----</span></span><span>
        </span><span><span class="hljs-comment"># Normalize</span></span><span>
        xt = self.norm1_temporal(x)  </span><span><span class="hljs-comment"># (B, T, N, C)</span></span><span>
        </span><span><span class="hljs-comment"># We want attention across time for each spatial index.</span></span><span>
        </span><span><span class="hljs-comment"># Rearrange to (B*N, T, C) then (T, B*N, C)</span></span><span>
        xt = xt.permute(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">3</span></span><span>).contiguous().view(B * N, T, C).permute(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>)  </span><span><span class="hljs-comment"># (T, B*N, C)</span></span><span>
        attn_out_temporal, _ = self.attn_temporal(xt, xt, xt)  </span><span><span class="hljs-comment"># (T, B*N, C)</span></span><span>
        attn_out_temporal = attn_out_temporal.permute(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>).contiguous().view(B, N, T, C).permute(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">3</span></span><span>)  </span><span><span class="hljs-comment"># (B, T, N, C)</span></span><span>
        </span><span><span class="hljs-comment"># Residual + MLP</span></span><span>
        x = x + attn_out_temporal
        x = x + self.mlp_temporal(self.norm2_temporal(x))

        </span><span><span class="hljs-keyword">return</span></span><span> x  </span><span><span class="hljs-comment"># (B, T, N, C)</span></span><span>
</span></span></code></div></div></pre>
